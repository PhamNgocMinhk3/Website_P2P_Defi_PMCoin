{
  "name": "client-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "cross-env NODE_OPTIONS=--max-old-space-size=16384 ng serve --disable-host-check --poll=2000",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=12288 ng build --configuration production",
    "watch": "cross-env NODE_OPTIONS=--max-old-space-size=8192 ng build --watch --configuration development",
    "test": "ng test",
    "clean": "rimraf dist .angular node_modules/.cache .tsbuildinfo",
    "clean:full": "npm run clean && npm install",
    "serve:ssr:client-angular": "node dist/client-angular/server/server.mjs",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/client-angular/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.1.0",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/material": "^20.1.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-server": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@angular/ssr": "^20.0.2",
    "@ctrl/ngx-emoji-mart": "^9.2.0",
    "@emoji-mart/data": "^1.2.1",
    "@microsoft/signalr": "^9.0.6",
    "aos": "^2.3.4",
    "chart.js": "^4.5.0",
    "emoji-mart": "^5.6.0",
    "ethers": "^6.15.0",
    "express": "^5.1.0",
    "gsap": "^3.13.0",
    "lightweight-charts": "^5.0.8",
    "ng2-charts": "^8.0.0",
    "rxjs": "~7.8.0",
    "three": "^0.165.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.0.2",
    "@angular/cli": "^20.0.2",
    "@angular/compiler-cli": "^20.0.0",
    "@types/aos": "^3.0.7",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "@types/three": "^0.165.0",
    "cross-env": "^10.0.0",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.8.2",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
